  <div class="container">



<h1>Login </h1>
    <%= button_to 'Google', '/auth/google_oauth2' %>

<%= form_with url: '/login',  local: true do |f| %>


<%= f.label :username %>
<%= f.text_field :username, autofocus: true %>
<br>
<%= f.label :password %>
<%= f.password_field :password %><br>

<%= f.submit :Submit, class: 'btn btn-default' %>
</div>
<% end %>



